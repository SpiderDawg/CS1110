Assignment 7

public class Node{
	String data;
	Node next;
	Node prev;
	
	public Node(String d, Node n, Node p){
		data = d;
		next = n;
		prev = p;
	}
	public String getData(){
		return data;
	}

	public Node getNext(){
		return next;
	}

	public Node getPrev(){
		return prev;
	}

	public void setData(String d){
		data = d;
	}

	public void setNext(Node n){
		next = n;
	}

	public void setPrev(Node p){
		prev = p;
	}
	
}